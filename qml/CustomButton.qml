import QtQuick 2.4
import Ubuntu.Components 1.3

Button {
    property alias icon: buttonIcon.name
    property alias iconWidth: buttonIcon.width
    property alias iconHeight: buttonIcon.height

    Icon {
        id: buttonIcon
        anchors.centerIn: parent
        color: UbuntuColors.darkGrey
        asynchronous: true
    }
    color: "transparent"
}
